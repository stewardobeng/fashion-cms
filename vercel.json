{
  \"version\": 2,
  \"builds\": [
    {
      \"src\": \"package.json\",
      \"use\": \"@vercel/next\"
    }
  ],
  \"env\": {
    \"DATABASE_URL\": \"@database_url\",
    \"JWT_SECRET\": \"@jwt_secret\",
    \"NODE_ENV\": \"production\"
  },
  \"functions\": {
    \"src/app/api/**/*.ts\": {
      \"maxDuration\": 30
    }
  }
}